# Qdrant Configuration
QDRANT_HOST=qdrant
QDRANT_PORT=6333

# LLM Configuration
# Set LLM_PROVIDER to either "gemini" (default) or "ollama"
LLM_PROVIDER=gemini

# Gemini Configuration (Required if LLM_PROVIDER=gemini)
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-1.5-flash

# Ollama Configuration (Required if LLM_PROVIDER=ollama)
OLLAMA_URL=http://ollama:11434
LLM_MODEL=llama3 